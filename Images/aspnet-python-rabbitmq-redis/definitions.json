{
    "vhosts": [
        {
            "name": "/"
        }
    ],
    "users": [
        {
            "name": "admin1",
            "password": "123456",
            "tags": "administrator"
        }
    ],
    "permissions": [
        {
            "user": "admin1",
            "vhost": "/",
            "configure": ".*",
            "write": ".*",
            "read": ".*"
        }
    ],
    "policies": [
        {
            "vhost": "/",
            "name": "lagoon-ha",
            "pattern": ".*",
            "definition": {
                "ha-mode": "exactly",
                "ha-params": 2,
                "ha-sync-mode": "automatic",
                "ha-sync-batch-size": 5
            }
        }
    ]
}